<Window x:Class="TradingJournalV2.ViewCommentsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TradingJournalV2"
        mc:Ignorable="d"
        Title="Comments" Height="450" Width="800">
  <Grid>
    <TextBox x:Name="tbComments" HorizontalAlignment="Left" Height="354" Margin="10,10,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="774" />
    <Button x:Name="btnSaveComments" Content="Save" HorizontalAlignment="Left" Margin="10,369,0,0" VerticalAlignment="Top" Width="113" Height="42" Click="btnSaveComments_Click"/>
  </Grid>
</Window>
